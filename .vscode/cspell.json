{
    "languageSettings": [
        {
            "languageId": "lean4",
            "includeRegExpList": [
                "--.*",                // single-line comments (-- ...)
                "//-[\\s\\S]*?-//",    // block comments (/- ... -/,
                                       // including /-- ... -/ and /-! ... -/)
            ],
            "ignoreRegExpList": [
                "`[^`]*`",             // words in single backticks
            ]
        }
    ],   
    
    "words": [
        "Yannick",
        "Seurin",
        "Mathlib",
        "Finset",
        "pointwise",
        "Bellare",
        "PKE",
        "ciphertext",
        "ciphertexts",
        "CDH",
        "DDH",
        "bijective",
        "distinguisher",
        "Diffie",
        "ElGamal"
    ]
}